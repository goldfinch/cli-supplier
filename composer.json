{
    "name": "goldfinch/cli-supplier",
    "description": "CLI Supplier is an extra interface for Taz commands that creates a bridge to communicate with Silverstripe core in CLI",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cli-supplier"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Art Karasev",
            "email": "art.karasev@proton.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "goldfinch/taz": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\CLISupplier\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
